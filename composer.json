{
    "name": "bamarni/symfony-http-cache-extension",
    "require": {
        "php": ">=5.3.2",
        "doctrine/cache": ">=1.*",
        "symfony/http-kernel": ">=2.*"
    },
    "require-dev": {
        "symfony/config": ">=2.*",
        "symfony/dependency-injection": ">=2.*"
    },
    "suggest": {
        "symfony/framework-bundle": "For integration with the framework"
    },
    "autoload": {
        "psr-0": {
            "Bamarni\\HttpCache": ""
        }
    },
    "target-dir": "Bamarni/HttpCache"
}
